

#include <stdio.h>
#include <stdlib.h>

retorno;

int main(void)
{
	int numero1;
	int numero2;
	int suma;


	printf("Imprimir un numero");
	scanf("%d", &numero1);

	printf("imprimir un numero");
	scanf("%d", &numero2);


}
int suma(int numero1,int numero2)
{
	suma = numero1 + numero2;
	if(suma != NULL)
	{
		retorno=1;
	}
	else
	{
		retorno=0;
	}
	retorno=1;
}


